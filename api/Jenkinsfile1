pipeline {    
       agent{
       label 'docker'
    }
    environment {
    DOCKERHUB_CREDENTIALS = credentials('dockerhub')
     registry = "padmaja11ravi/backend-lms"
        registryCredential = 'dockerhub'
        dockerImage = ''
    }
}
 stages {
        
         stage('creating  new docker network') {
            steps {
               
                sh 'docker network create lmsnetwork'
            }
        } 
}
